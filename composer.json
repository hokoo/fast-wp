    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "require": {
    "johnpbloch/wordpress-core-installer": "*",
    "composer/installers": "^1.0@dev",
    "johnpbloch/wordpress": "^5",
    "mobiledetect/mobiledetectlib": "^2.8",
    "wpackagist-plugin/classic-editor": "^1.5",
    "wpackagist-plugin/wp-smtp": "^1.1",
    "johnbillion/query-monitor": "^3.7",
    "wpackagist-plugin/woocommerce": "^6.5",
    "wpackagist-plugin/contact-form-7": "5.5.6.1",
    "wpackagist-plugin/wp-data-logger": "^2.0",
    "wpackagist-plugin/custom-post-type-ui": "^1.12"
  },
  "autoload": {
  },
  "extra": {
    "installer-paths": {
      "wp-content/plugins/{$name}": [
        "type:wordpress-plugin"
      ]
    }
  },
  "scripts": {
    "pre-install-cmd": [],
    "post-install-cmd": []
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "johnpbloch/wordpress-core-installer": true
    }
  }
}
